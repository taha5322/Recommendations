<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.siddiqui.recommendations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".android.ui.businessregistration.BusinessRegistrationActivity"
            android:parentActivityName=".android.MainActivity"/>
        <activity android:name=".android.ui.businesslist.BusinessListActivity"
            android:parentActivityName=".android.MainActivity"/>
        <activity android:name=".android.ui.businessdetail.BusinessDetailActivity"
            android:parentActivityName=".android.MainActivity"/>
        <activity android:name=".android.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>